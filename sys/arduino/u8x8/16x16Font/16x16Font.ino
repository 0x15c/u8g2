*/
void set16x16Fonts(const uint8_t *ul, const uint8_t *ur, const uint8_t *ll, const uint8_t *lr)
{
  u8x8_font_ul = ul;
  u8x8_font_ur = ur;
  u8x8_font_ll = ll;
  u8x8_font_lr = lr;
}

/* draw a single char */
void draw16x16Glyph(uint8_t x, uint8_t y, uint8_t encoding)
{
  u8x8.setFont(u8x8_font_ul);
  u8x8.drawGlyph(x, y, encoding);
  u8x8.setFont(u8x8_font_ur);
  u8x8.drawGlyph(x+1, y, encoding);
  u8x8.setFont(u8x8_font_ll);
  u8x8.drawGlyph(x, y+1, encoding);
  u8x8.setFont(u8x8_font_lr);
  u8x8.drawGlyph(x+1, y+1, encoding);
}

/* draw a single char with double height */
void draw16x32Glyph(uint8_t x, uint8_t y, uint8_t encoding)
{
  u8x8.setFont(u8x8_font_ul);
  u8x8.draw1x2Glyph(x, y, encoding);
  u8x8.setFont(u8x8_font_ur);
  u8x8.draw1x2Glyph(x+1, y, encoding);
  u8x8.setFont(u8x8_font_ll);
  u8x8.draw1x2Glyph(x, y+2, encoding);
  u8x8.setFont(u8x8_font_lr);
  u8x8.draw1x2Glyph(x+1, y+2, encoding);
}

/* draw a string */
void draw16x16String(uint8_t x, uint8_t y, const char *s)
{
  while( *s != '\0' )
  {
    draw16x16Glyph(x, y, *s);
    x+=2;
    s++;
  }    
}

/* draw a string (double height) */
void draw16x32String(uint8_t x, uint8_t y, const char *s)
{
  while( *s != '\0' )
  {
    draw16x32Glyph(x, y, *s);
    x+=2;
    s++;
  }    
}


