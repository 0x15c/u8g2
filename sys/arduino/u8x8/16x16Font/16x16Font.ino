  set16x16Fonts(u8x8_font_px437wyse700a_ul_n, u8x8_font_px437wyse700a_ur_n, u8x8_font_px437wyse700a_ll_n, u8x8_font_px437wyse700a_lr_n);
  set16x16Fonts(u8x8_font_px437wyse700b_ul_n, u8x8_font_px437wyse700b_ur_n, u8x8_font_px437wyse700b_ll_n, u8x8_font_px437wyse700b_lr_n);

  ASCII range 32..127
  set16x16Fonts(u8x8_font_px437wyse700a_ul_r, u8x8_font_px437wyse700a_ur_r, u8x8_font_px437wyse700a_ll_r, u8x8_font_px437wyse700a_lr_r);
  set16x16Fonts(u8x8_font_px437wyse700b_ul_r, u8x8_font_px437wyse700b_ur_r, u8x8_font_px437wyse700b_ll_r, u8x8_font_px437wyse700b_lr_r);

